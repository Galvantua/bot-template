{
	"name": "bot-template",
	"version": "1.0.0",
	"description": "this is just a basic discord.js bot template",
	"main": "src/index.mjs",
	"scripts": {
		"test": "echo \"Error: no test specified\" && exit 1",
		"start": "cd src&& SET NODE_ENV=production&&node --no-warnings --experimental-json-modules index.mjs",
		"start:dev": "cd src&& SET NODE_ENV=dev&&node --experimental-json-modules index.mjs",
		"start:dev:mon": "cd src&& SET NODE_ENV=dev&&nodemon --experimental-json-modules index.mjs"
	},
	"author": "Gavin Morton",
	"license": "ISC",
	"dependencies": {
		"@discordjs/rest": "^1.1.0",
		"discord.js": "^14.3.0",
		"fuse.js": "^6.6.2",
		"node-fetch": "^2.6.1",
		"prettier": "^2.8.0"
	},
	"prettier": {
		"singleQuote": true,
		"trailingComma": "all",
		"printWidth": 80,
		"tabWidth": 4,
		"useTabs": true,
		"semi": true,
		"bracketSpacing": true
	}
}
